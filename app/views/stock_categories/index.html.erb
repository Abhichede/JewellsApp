<div class="todays_rate">
  <% @stock_categories.each do |category| %>
    <% if category.updated_at < Date.today.to_time %>
      <h5>
        <span class="label label-danger">
          You need to enter Today's Rate for <%= category.category_name %>
        </span>
        &nbsp; &nbsp; Last entered on: <%= Time.parse(category.updated_at.to_s).strftime('%d-%b-%Y %I:%M %p') %>
      </h5>
    <% end %>
  <% end %>
</div>
<div id="stock_categories"></div>
<script type="text/javascript" charset="utf-8">
    $(function () {
        window.router = new JewellsApp.Routers.StockCategoriesRouter({stock_categories: <%= @stock_categories.to_json.html_safe %>});
    });
</script>